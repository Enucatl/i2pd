[ntcp2]
## Enable NTCP2 transport (default = true)
# enabled = true
<% unless $enabled =~ Undef { -%>
enabled = <%= $enabled %>
<% } -%>
## Publish address in RouterInfo (default = true)
# published = true
<% unless $published =~ Undef { -%>
published = <%= $published %>
<% } -%>
## Port for incoming connections (default is global port option value)
# port = 4567
<% unless $port =~ Undef { -%>
port = <%= $port %>
<% } -%>
